{
  "name": "{{PROJECT_NAME}}",
  "private": true,
  "type": "module",
  "workspaces": ["apps/*", "packages/*", "modules/*", "scripts/*"],
  "scripts": {
    "build": "moon run :build",
    "dev": "moon run :dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "moon run :test",
    "test:watch": "vitest",
    "clean": "moon run :clean"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^9.1.0",
    "lefthook": "^1.10.0",
    "prettier": "^3.8.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.0"
  }
}
